{
    "[python]": {
        "editor.defaultFormatter": "ms-python.python",
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports.ruff": "explicit"
        }
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true
    },
    "editor.formatOnSave": true,
    "prettier.configPath": "./frontend/.prettierrc",
    "cSpell.words": [
        "Asics",
        "autoflake",
        "autoflush",
        "camelcase",
        "cellspacing",
        "colorlog",
        "decamelization",
        "decamelize",
        "decamelized",
        "dekebabize",
        "depascalize",
        "dotenv",
        "drpi",
        "fastapi",
        "heroicons",
        "hzne",
        "ilike",
        "initialisms",
        "isnot",
        "jsonable",
        "kebabcase",
        "kebabize",
        "kebabized",
        "levelname",
        "ondelete",
        "onupdate",
        "openai",
        "pascalcase",
        "pascalize",
        "pascalized",
        "passlib",
        "Popins",
        "Pydantic",
        "pytz",
        "rsps",
